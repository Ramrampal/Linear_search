#include<iostream>
using namespace std;
int main(){
    int arr[]={4,5,34,65,7,34,3,5,};
    int n = sizeof(arr)/sizeof(arr[0]);
    int x;
    cout<<"Enter the target  ";
    cin>>x;
    for(int i=0;i<n;i++){
        if( arr[i]==x){
            cout<<x<<" is present";
            break;
        }
    }
}
